<%= inline_editable :where, space: @space  do %>

  <div class="flex gap-1 mb-3">
    <%= inline_svg_tag 'place' %>
    <span>
      <%= @space.address %>,
      <%= @space.post_number %>
      <%= @space.post_address %>
    </span>
  </div>
  <%= render partial: "spaces/show/location_description"%>
  <%= render partial: "spaces/show/static_map" %>

<% end %>
