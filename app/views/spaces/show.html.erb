    <div class="flex gap-4">

      <div class="star flex gap-2">
        <%= inline_svg_tag 'place' %>
        <span>
          <%= @space.address %>
        </span>
      </div>

    </div>
  <p>Latitude: <%= @space.lat %></p>
  <p>Longitude: <%= @space.lng %></p>
</div>
