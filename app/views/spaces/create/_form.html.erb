<%= simple_form_for space do |form| %>
  <%= edit_field 'title', form %>
  <%= edit_field 'space_type', form %>
  <%= edit_field 'where', form %>
  <%= edit_field 'space_group', form %>
  <span class="flex gap-4 items-baseline mt-4">
  <%= form.button :submit %>
      <%= link_to t('simple_form.labels.user.back'), :back %>
  </span>
<% end %>
